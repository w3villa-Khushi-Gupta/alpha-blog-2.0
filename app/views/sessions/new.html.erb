<p id="alert"><%= alert %></p>
<h1>Login</h1>

<%= form_with(scope: :session, url: login_path ) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email%>
  </div>


  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div class="actions">
    <%= form.submit "Log In" %>
  </div>

<% end %>
 